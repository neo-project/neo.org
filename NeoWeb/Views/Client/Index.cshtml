                        <span>@Localizer["Supported NEP"]</span><span class="span-danger">@Localizer["Poor"]</span>
                        <div>
                            @Localizer["Fail to support more NEP"]
                        </div>
                    </div>
                </div>
            </div>
        </li>
    </ul>
</div>

<div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="clientModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="modal-hr">
                    <div class="clearfix">
                        <img class="float-left" id="model_img" />
                        <div class="modal-title float-left">
                            <h3 id="model_name"></h3>
                            <div id="model_dev"></div>
                            <div id="model_button"></div>
                            <div id="model_license" class="light-text"></div>
                        </div>
                    </div>
                    <div class="developer" id="model_platform"></div>
                    <div class="light-text">Supported Platform</div>
                </div>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="model_desc"></div>
        </div>
    </div>
</div>
@section Scripts{
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $(".client-ul li img").click(function () {
                zoomin(this);
            });
            $(".client-ul li header").click(function () {
                zoomin(this);
            });
            $(".card-header").attr("onclick", "showDetails(this)");
            if (window.location.hash) {
                var _num = parseInt(window.location.hash.substr(1, 1));
                var _str = window.location.hash.substr(2);
                console.log(_num);
                console.log(_str);
                showPlatform($('.nav-client li')[_num], _str);
            }
        })
        $("#select").change(function () {
            showPlatform($(this).val());
        });
        function showPlatform(name) {
            $("body").trigger("scroll");
            if (name) {
                $('.client-ul li').not('.' + name).hide();
                $('.' + name).show();
            }
            else
                $('.client-ul li').show();
        }
        function switchPlatform(obj) {
            $(".modal .preview").hide();
            $(".modal .p-" + obj).show();
            $(".modal .plat-btn").removeClass("active");
            $(".modal .btn" + obj).addClass("active");
        }
        function zoomin(obj) {
            var img = $(obj).parent().find("img");
            var name = $(obj).parent().find("header");
            var developer = $(obj).parent().find(".developer");
            var platform = $(obj).parent().find(".platform");
            var desc = $(obj).parent().find(".desc");
            var link = $(obj).parent().find(".link");
            var button = $(obj).parent().find(".button");
            var license = $(obj).parent().find(".license");
            $("#modal").modal();
            $("#model_img").html("");
            $("#model_name").html("");
            $("#model_dev").html("");
            $("#model_platform").html("");
            $("#model_button").html("");
            $("#model_license").html("");
            $("#model_desc").html("");
            $("#model_link").html("");
            desc.find("img").each(function () {
                $(this).attr("src", $(this).data("original"));
            });;

            $("#model_img").attr("src", img.attr("src"));
            $("#model_name").html(name.html());
            $("#model_dev").html(developer.html());
            $("#model_platform").html(platform.html());
            $("#model_button").html(button.html());
            $("#model_license").html(license.html());
            $("#model_desc").html(desc.html());
            $("#model_link").html(link.html());
        }
        $("#menu_list a").css("border-bottom", "none")
        $("#menu_list li.nav-item [href='/client']").css("border-bottom", "2px solid #58bf00")
    </script>
}