@section Scripts{
    <script>
        $(function () {
            var test = window.location.hash.substr(1);
            var num = parseInt(test);
            $("#downTab li").eq(num ? num - 1 : 0).addClass("downtab-select");
            $(".download-container ul").eq(num ? num - 1 : 0).addClass("down-select");

            $("#downTab li").click(function () {
                $(".downtab-select").removeClass("downtab-select");
                $(".down-select").removeClass("down-select");
                $(this).addClass("downtab-select");
                $(".download-container ul").eq($(this).index()).addClass("down-select");
            })
        })
        $("#menu_list a").css("border-bottom", "none")
        $("#menu_list li.nav-item [href='/client']").css("border-bottom", "2px solid #58bf00")
    </script>
