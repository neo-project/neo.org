@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer

@{
    ViewData["Title"] = Localizer["Get Client"].Value;
}
@section Styles{
    <environment include="Development">
        <link rel="stylesheet" href="/css/client.css" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="@Helper.ToCDN("/css/client.min.css", true)" />
    </environment>
}
<div class="container">
    <div class="p1">
        <h1 class="w800">@Localizer["NEO & GAS"]</h1>
        <p class="mt5 w600">@Localizer["The wallets shown on this page are all developed by the development teams or individual developers of the NEO community. Please contact the developer if you encounter any problems during use."]</p>
        <p class="mt5 w600">@Localizer["As some wallets are open-source while some are not, you may lose control of your own NEO assets when using some wallets that shown on this page. Please choose the wallets as appropriate."]</p>
        <p class="mt5 w600">@Localizer["When using the wallet, please back up the private key in a secure manner. The private key is the only way to control your own NEO assets."]</p>
    </div>

    <h2 class="m2">@Localizer["Get Client"]</h2>
    <div>
        <p class="mt5">@Localizer["The wallets shown on this page are all developed by the development teams or individual developers of the NEO community. Please contact the developer if you encounter any problems during use."]</p>
        <p class="mt5">@Localizer["As some wallets are open-source while some are not, you may lose control of your own NEO assets when using some wallets that shown on this page. Please choose the wallets as appropriate."]</p>
        <p class="mt5">@Localizer["When using the wallet, please back up the private key in a secure manner. The private key is the only way to control your own NEO assets."]</p>
    </div>
    <select id="select">
        <option value="">@Localizer["All Client"]</option>
        <option value="desktop">@Localizer["Desktop"]</option>
        <option value="mobile">@Localizer["Mobile"]</option>
        <option value="web">@Localizer["Web"]</option>
        <option value="extension">@Localizer["Extension"]</option>
        <option value="hardware">@Localizer["Hardware"]</option>
    </select>
    <ul class="row mt5 client-ul">
        <li class="desktop">
            <img class="client-logo" data-original="@Helper.ToCDN("/images/client/logo/gui.png")" />
            <header>NEO-GUI</header>
            <div class="platform">
                <i class="iconfont" data-toggle="tooltip" data-placement="bottom" title="Windows">&#xe898;</i>
            </div>
            <a class="open-source" href="https://github.com/neo-project/neo-gui" target="_blank">GitHub</a>
            <div class="developer d-none"><a href="https://github.com/neo-project/neo-gui" target="_blank">NEO Project</a></div>
            <div class="license d-none"><a class="float-left" href="https://github.com/neo-project/neo-gui/blob/master/LICENSE" target="_blank">MIT License</a></div>
            <div class="button d-none"><a class="btn" href="https://github.com/neo-project/neo-gui/releases" target="_blank">@Localizer["Download"]</a></div>
            <div class="desc d-none">
                <div class="preview-div">
                    <h3>Preview</h3>
                    <a class="plat-btn btn-desktop active" href="javascript:" onclick="switchPlatform('desktop');">@Localizer["Desktop"]</a>
                </div>
                <div class="p-desktop preview">
                    <img data-original="@Helper.ToCDN("/images/client/gui.png")" />
                </div>
                <p>@Localizer["NEO-GUI is a full-node NEO client, which contains all the basic functions of NEO network. This wallet is designed for advanced users, with professional software interaction and extremely high security. However, it takes a long time to initialize synchronization, occupies disk and memory, and has poor stability."]</p>
                <p>@Localizer["Need to install"] <a href="https://www.microsoft.com/net/download/dotnet-framework-runtime" target="_blank">.NetFramework 4.7.1</a> @Localizer["and"] <a href="https://www.microsoft.com/en-us/download/details.aspx?id=14632" target="_blank">VC++ 2010 Redist x64</a>/<a href="https://www.microsoft.com/en-us/download/details.aspx?id=5555" target="_blank">x86</a>
                <p>@Localizer["For more information, see:"]<a href="@Localizer["https://docs.neo.org/en-us/node/gui/install.html"]" target="_blank">@Localizer["Instructions for use"]</a></p>
                <p>@Localizer["Languages"]: English / 简体中文</p>
                <div>
                    <div class="metrics">
                        <span>@Localizer["Degree of Digital Asset Control"]</span><span class="span-success">@Localizer["High"]</span>
                        <div>@Localizer["This wallet gives users a high degree of control over digital assets. The private key is encrypted and stored by the wallet files and passwords. However, users should better run the wallet on secure devices, and always pay attention to the safe backup and safekeeping of the private key."]</div>
                    </div>
                    <div class="metrics">
                        <span>@Localizer["Verification"]</span><span class="span-success">@Localizer["Comprehensive"]</span>
                        <div>@Localizer["This wallet is a full node that stores all the data in the NEO blockchain, which means that there is no need to trust a third party when verifying payments or executing contracts. The full node provides the highest level of security and is an integral part of protecting the network. However, it requires more hard disk space (15GB), greater bandwidth, and a longer initialization synchronization time. The initialization synchronization speed can be accelerated by downloading the <a href=\"http://sync.ngd.network/\" target=\"_blank\">offline synchronization package</a> provided by NGD."]</div>
                    </div>
                    <div class="metrics">
                        <span>@Localizer["Transparency"]</span><span class="span-success">@Localizer["High"]</span>
                        <div>@Localizer["This wallet is open-source, which means any developer in the world can audit the code and make sure the final software isn't hiding any secrets."]@Localizer["Get source code"]: <a href="https://github.com/neo-project/neo-gui" target="_blank">GitHub</a></div>
                    </div>
                    <div class="metrics">
                        <span>@Localizer["Support Plugins"]</span>
                        <p>NEO-GUI @Localizer["supports plugins. The following is a list of commonly-used plug-in components."]</p>
                        <table class="table table-bordered table-sm">
                            <thead>
                                <tr>
                                    <th scope="col">@Localizer["Plugin"]</th>
                                    <th scope="col">@Localizer["Introduction"]</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th scope="row">
                                        <a href="https://github.com/neo-project/neo-plugins/releases/" target="_blank">ImportBlocks</a>
                                    </th>
                                    <td>@Localizer["Synchronizes the client using offline packages. Installed by default."]</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="metrics">
                        <span>@Localizer["Supported NEP"]</span><span class="span-success">@Localizer["Rich"]</span>
                        <div>
                            <p>@Localizer["Support"]: <a href="https://github.com/neo-project/proposals/blob/master/nep-5.mediawiki" target="_blank">NEP-5</a>, <a href="https://github.com/neo-project/proposals/blob/master/nep-6.mediawiki" target="_blank">NEP-6</a> </p>
                        </div>
                    </div>
                </div>
            </div>
        </li>
        <li class="desktop">
            <img class="client-logo" data-original="@Helper.ToCDN("/images/client/logo/cli.png")" />
            <header>NEO-CLI</header>
            <div class="platform">
                <i class="iconfont" header data-toggle="tooltip" data-placement="bottom" title="Windows">&#xe898;</i>
                <i class="iconfont" data-toggle="tooltip" data-placement="bottom" title="Linux">&#xf1e8;</i>
                <i class="iconfont" data-toggle="tooltip" data-placement="bottom" title="macOS">&#xe61b;</i>
            </div>
            <a class="open-source" href="https://github.com/neo-project/neo-cli" target="_blank">GitHub</a>
            <div class="developer d-none"><a href="https://github.com/neo-project/neo-cli" target="_blank">NEO Project</a></div>
            <div class="license d-none"><a href="https://github.com/neo-project/neo-cli/blob/master/LICENSE" target="_blank">MIT License</a></div>
            <div class="button d-none"><a class="btn" href="https://github.com/neo-project/neo-cli/releases" target="_blank">@Localizer["Download"]</a></div>
