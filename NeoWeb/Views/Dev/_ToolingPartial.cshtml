@using Microsoft.AspNetCore.Mvc.Localization

@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

<div class="menu-page-title">@Localizer["Tooling"]</div>

<div class="tabs">
    <span class="tab active">@Localizer["Nodes"]</span>
    <span class="tab">@Localizer["Explorers"]</span>
    <span class="tab">@Localizer["Smart Contract Compilers"]</span>
    <span class="tab">@Localizer["Smart Contract Dev & Debug"]</span>
    <span class="tab">@Localizer["SDK"]</span>
    <span class="tab">@Localizer["dApp Integration"]</span>
</div>

<select class="select-tab-page form-control">
    <option selected="selected">@Localizer["Nodes"]</option>
    <option>@Localizer["Explorers"]</option>
    <option>@Localizer["Smart Contract Compilers"]</option>
    <option>@Localizer["Smart Contract Dev & Debug"]</option>
