@{
    ViewData["Title"] = "站点地图";
}
<style>
    .layout-container {
        max-width: none;
        padding: 0;
        margin: 0;
    }

    .map-container {
        background-color: white;
        color: #7f7f7f;
    }

        .map-container a {
            display: inline-block;
            margin: .5rem 1rem 0.2rem 1rem;
            color: #1B1B1B !important;
            letter-spacing: 1px;
            min-width: 130px;
        }

        .map-container span {
            background: #02e49b;
            margin-right: .5em;
            font-size: .7em;
            color: #1B1B1B;
            padding: 2px 1em;
            vertical-align: 1px;
        }

    hr {
        margin-top: 0.8rem;
        margin-bottom: 0.8rem;
    }
</style>
<div class="p1 bg3">
    <h1 class="w800">站点地图</h1>
</div>
