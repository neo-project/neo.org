@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

<div class="menu-page-title">@Localizer["Overview"]</div>

<div class="menu-page-introduction clearfix">
    <img class="menu-page-introduction-img" src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/overview.png", true)" />
    <div class="menu-page-title">@Localizer["Overview"]</div>
    <div class="menu-page-introduction-title">
        @Localizer["Our Nature is Nurture"]
    </div>
    <div class="menu-page-introduction-description">
        <p>@Localizer["Neo has initiated a number of support tracks to grow the Neo ecosystem, offering various types of assistance depending on your project needs, interests, and maturity. Options are available for grassroots developers, start up projects, established businesses, institutions, and everything in between."]</p>
        <p>@Localizer["If you would  benefit from a little help during your journey on the Neo blockchain, simply identify the track below that best describes your current situation. Full details on how to apply can be found by following the relevant links."]</p>
    </div>
</div>
<div class="menu-page-divider"></div>
<div class="menu-page-secondary-title">
    @Localizer["Find the support that is right for you"]
</div>
<div class="menu-page-tertiary-title">
    <img class="menu-page-title-img" src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/1.svg", true)" />
    @Localizer["Existing dApps or blockchain - related business"]
</div>

<div class="row py-4" style="background-image: url(@Helper.ToCDN("/images/eco/overview-bg.svg")); background-size: cover; background-position: center;">
    <div class="col-lg-6 my-3">
        <div class="card mx-auto" style="max-width: 25rem;">
            <div class="card-img-top">
                <img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/ecoboost.svg")" />
                <h4><strong>ECOBOOST</strong></h4>
            </div>
            <div class="card-body">
                <div class="text-center">
                    <strong class="card-title">@Localizer["Suitable for the following:"]</strong>
                </div>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["A small team with many seed users"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["A medium sized business"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["An institution or education platform"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["A wallet or trading platform with existing users"]</span></p>
                <div class="text-center my-3">
                    <a asp-controller="eco" asp-action="index" asp-fragment="ecoboost" class="btn-4 my-3 my-auto">@Localizer["LEARN MORE"]</a>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-6 my-3">
        <div class="card mx-auto" style="max-width: 25rem;">
            <div class="card-img-top">
                <img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/ecoboost.svg")" />
                <h4><strong>ECOFUND</strong></h4>
            </div>
            <div class="card-body">
                <div class="text-center">
                    <strong class="card-title">@Localizer["Suitable for the following:"]</strong>
                </div>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["An established blockchain-based business with substantial ties to the Neo ecosystem"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["A project operating with a viable revenue model"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["Teams looking for funding in exchange for equity or tokens"]</span></p>
                <div class="text-center my-3">
                    <a asp-controller="eco" asp-action="index" asp-fragment="ecofund" class="btn-4 my-3 my-auto">@Localizer["LEARN MORE"]</a>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-6 my-3">
        <div class="card mx-auto" style="max-width: 25rem;">
            <div class="card-img-top">
                <img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/earlyadoption.svg")" />
                <h4><strong>EARLY ADOPTION PROGRAM</strong></h4>
            </div>
            <div class="card-body">
                <div class="text-center">
                    <strong class="card-title">@Localizer["Suitable for the following:"]</strong>
                </div>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["Projects looking to migrate to Neo N3"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["Teams looking to deploy first-to-market dApps on Neo N3"]</span></p>
                <p class="card-text"><img src="@Helper.ToCDN("/images/transparent.png")" data-original="@Helper.ToCDN("/images/eco/check.svg")" /><span>@Localizer["Developers looking to create early POCs that test Neo N3 capabilities"]</span></p>
                <div class="text-center my-3">
                    <a asp-controller="eco" asp-action="index" asp-fragment="earlyadoption" class="btn-4 my-3 my-auto">@Localizer["LEARN MORE"]</a>
                </div>
            </div>
        </div>
    </div>
</div>
