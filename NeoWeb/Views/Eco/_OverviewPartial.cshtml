@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

<div class="menu-page-title">@Localizer["Overview"]</div>

<div class="menu-page-introduction clearfix">
    <img class="menu-page-introduction-img" data-original="@Helper.ToCDN("/images/eco/overview.png", true)" />
    <div class="menu-page-title">@Localizer["Overview"]</div>
    <div class="menu-page-introduction-title">
        @Localizer["Our Nature is Nuture"]
    </div>
    <div class="menu-page-introduction-description">
        <p>@Localizer["Neo has initiated a number of support tracks to grow the Neo ecosystem, offering various types of assistance depending on your project needs, interests, and maturity. Options are available for grassroots developers, start up projects, established businesses, institutions, and everything in between."]</p>
        <p>@Localizer["If you would  benefit from a little help during your journey on the Neo blockchain, simply identify the track below that best describes your current situation. Full details on how to apply can be found by following the relevant links."]</p>
    </div>
</div>
<div class="menu-page-divider"></div>
<div class="menu-page-secondary-title">
    @Localizer["Find the support that is right for you"]
</div>
<div class="menu-page-tertiary-title">
    @Localizer["Existing dApps or blockchain - related business"]
</div>
<div class="supports">
    <div class="support">
        <div class="support-i-am">@(SharedLocalizer["en"].Value == "en" ? "I am" : "如果你是")</div>
        <div class="support-start">
            <div class="support-start-items">
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["A small team with many seed users"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["A medium sized business"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["A wallet or trading platform with existing users"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["An institution or education platform"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["Looking for full lifecycle support"]</span>
                </div>
            </div>
        </div>
        <div class="support-end">
            <img class="support-end-img" data-original="@Helper.ToCDN("/images/eco/overview-eco-boost.png", true)" />
            <div class="support-end-text">@Localizer["Get all the support and guidence you need with EcoBoost"]</div>
            <a class="support-end-btn btn-4 btn-small">@Localizer["EcoBoost"]</a>
        </div>
    </div>
    <div class="support">
        <div class="support-i-am">@Localizer["I am"]</div>
        <div class="support-start">
            <div class="support-start-items">
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["An established blockchain-based business with substantial ties to the Neo ecosystem"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["Operating with a viable revenue model"]</span>
                </div>
                <div class="support-start-item">
                    <img class="support-start-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                    <span class="support-start-item-text">@Localizer["Looking for funding in exchange for equity or tokens"]</span>
                </div>
            </div>
        </div>
        <div class="support-end">
            <img class="support-end-img" data-original="@Helper.ToCDN("/images/eco/overview-eco-fund.png", true)" />
            <div class="support-end-text">@Localizer["Get all the funding aid you need with EcoFund"]</div>
            <a class="support-end-btn btn-4 btn-small">@Localizer["EcoFund"]</a>
        </div>
    </div>
</div>
