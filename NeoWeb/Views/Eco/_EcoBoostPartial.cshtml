@using Microsoft.AspNetCore.Mvc.Localization

@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

<div class="menu-page-title">
    <img class="menu-page-title-img" data-original="@Helper.ToCDN("/images/eco/eco-boost-title.png", true)" />
    <span class="menu-page-title-text">
        @Localizer["EcoBoost"]
    </span>
</div>
<div class="menu-page-introduction clearfix">
    <img class="menu-page-introduction-img" data-original="@Helper.ToCDN("/images/eco/eco-boost.png", true)" />
    <div class="menu-page-title">
        <img class="menu-page-title-img" data-original="@Helper.ToCDN("/images/eco/eco-boost-title.png", true)" />
        <span class="menu-page-title-text">
            @Localizer["EcoBoost"]
        </span>
    </div>
    <div class="menu-page-introduction-title">
        @Localizer["Working together"]
    </div>
    <div class="menu-page-introduction-description">
        <p><strong>@Localizer["Working in tandem with various partners, the EcoBoost program provides full lifecycle support for high-potential projects."]</strong></p>
        <p>@Localizer["First announced at Consenus 2019, EcoBoost is a US $100 million intiative designed to grow the Neo ecosystem."]</p>
        <p>@Localizer["Operated by the Neo Global Development EcoGrowth department, the program works by building up a partner network to provide assistance to Neo-based projects."]</p>
        <p>@Localizer["EcoBoost partners are leveraged to provide full lifecycle support to Neo projects, based on the individual requirements of each project. For example, media partners can assist with outreach and publicity, exchange partners can help list tokens and stimulate liquidity, and institutional partners can provide industry resources."]</p>
        <p>@Localizer["EcoBoost partners will benefit from business growth opportunities provided by access to the Neo ecosystem and its global community."]</p>
        <p>@Localizer["The Neo Global Development EcoGrowth team will work with EcoBoost partners and projects to ensure mutially beneficial outcomes for all participants."]</p>
    </div>
</div>

<div class="menu-page-divider"></div>
<div class="menu-page-secondary-title">
    @Localizer["How to apply"]
</div>
<div class="menu-page-description">
    <strong>@Localizer["Projects or organizations that believe they fit the scope of the EcoBoost program are welcome to apply."]</strong>
</div>
<div class="menu-page-tertiary-title">
    @Localizer["projects"]
</div>
<div class="application">
    <div class="application-start">
        <div class="application-start-title">
            @Localizer["If you are…"]
        </div>
        <ul class="application-start-items">
            <li>
                @Localizer["A Project or business within the Neo ecosystem"]
            </li>
            <li>
                @Localizer["A Project or business looking to migrate to, or integrate Neo"]
            </li>
            <li>
                @Localizer["A Small team with innovative ideas and seed users, or"]
            </li>
            <li>
                @Localizer["A Medium sized teams with a mature business model and existing community / customer base"]
            </li>
        </ul>
    </div>
    <div class="application-end">
        <i class="application-chevron iconfont">&#xe6ab;</i>
        <div class="application-end-title">
            @Localizer["EcoBoost could provide…"]
        </div>
        <div class="application-end-items">
            <div class="application-end-item">
                <img class="application-end-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                <span class="application-end-item-text">
                    @Localizer["Access to the network of EcoBoost partners for tailored support"]                 </span>
            </div>
            <div class="application-end-item">
                <img class="application-end-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                <span class="application-end-item-text">
                    @Localizer["Full lifecycle support, including funding, technology support, community access, and marketing assistance"]
                </span>
            </div>
            <div class="application-end-item">
                <img class="application-end-item-img" data-original="@Helper.ToCDN("/images/eco/checked.png", true)" />
                <span class="application-end-item-text">
                    @Localizer["Funding of up to US $50,000"]
                </span>
            </div>
        </div>
    </div>
</div>
<div class="requirements-title">
    @Localizer["Application requirements"]
</div>
<div class="menu-page-description">
    <strong>@Localizer["If you feel your project would benefit from EcoBoost support, please contact NGD EcoGrowth with the following information:"]</strong>
</div>
<ul class="requirements">
    <li>
        @Localizer["Project or company name"]
    </li>
    <li>
        @Localizer["Team size, names, experience, and location"]
    </li>
    <li>
        @Localizer["Detailed information outlining:"]
        <ul class="requirements-sublist">
            <li>
                @Localizer["The nature of your project or business"]
            </li>
            <li>
                @Localizer["Milestones you have achieved, along with short, medium, and long term goals"]
            </li>
            <li>
                @Localizer["Your unique value proposition"]
            </li>
            <li>
                @Localizer["Challenges you see in the short to medium term"]
            </li>
            <li>
                @Localizer["Size of current user or customer base"]
            </li>
        </ul>
    </li>
    <li>
        @Localizer["How you think EcoBoost can help you achieve your goals and what kind of support you are looking for"]
    </li>
    <li>
        @Localizer["Links to your website or application"]
    </li>
    <li>
        @Localizer["Main contact details"]
    </li>
</ul>
<div class="apply">
    <div class="apply-text">@Localizer["Apply for a Project EcoBoost now, NGD EcoGrowth will generally respond within one month."]</div>
    <a class="apply-btn btn-2 btn-small" href="mailto:eg@neo.org">
        @Localizer["Apply Now"]&nbsp;&nbsp;<i class="iconfont">&#xe6ab;</i>
    </a>
</div>
