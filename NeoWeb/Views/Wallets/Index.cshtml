            <div class="modal-content">
                <div class="modal-header">
                    <div class="modal-hr">
                        <div class="clearfix">
                            <img class="float-left" id="model_img" />
                            <div class="modal-title float-left">
                                <h3 id="model_name"></h3>
                                <div id="model_dev"></div>
                                <div id="model_button"></div>
                                <div id="model_license" class="light-text"></div>
                            </div>
                        </div>
                        <div class="platform" id="model_platform"></div>
                        <div class="light-text">@Localizer["Supported Platform"]</div>
                    </div>
