@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = "Completed";
}
@section Styles{
    <environment include="Development">
        <link rel="stylesheet" href="/css/testnet.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="@Helper.CDN/css/testnet.min.css" asp-append-version="true" />
    </environment>
}
<div class="container completed-content">
    <h1 class="apply-title m2">@Localizer["The application has been submitted"]</h1>
    <div class="alert-text">
        @Localizer["Your NEO/GAS application has been submitted, and if approved, we will send NEO/GAS to a multiple-signature address within 3 business days and notify you by email of how to add the address to the NEO wallet and how to use it. Please wait patiently."]
    </div>
</div>

