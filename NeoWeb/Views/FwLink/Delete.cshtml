@model NeoWeb.Models.FwLink

@{
    ViewData["Title"] = "Delete FwLink";
}

<p class="mt-4">Are you sure you want to delete this?</p>
    <p class="mt5 w600">@Html.DisplayFor(model => model.Name)</p>
    <p class="mt5 w600" style="word-break: break-all">@Html.DisplayFor(model => model.Link)</p>
    <div class="mt5">
        <div class="mt-3">
            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <div class="my-4">
                    <input type="submit" value="Delete" class="btn mt5" />
                </div>
                <div class="my-4">
                    <a asp-action="list">Back to List</a>
                </div>
            </form>
        </div>
    </div>
